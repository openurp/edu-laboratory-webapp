[#ftl]
[@b.grid items=applyList var="apply" sortable="false"]
	[@b.row]
		[@b.col width="4%" title="序号"]${apply_index+1}[/@]
		[@b.col width="5%" property="no" title="attr.taskNo"]${apply.lesson.no?if_exists}[/@]
		[@b.col width="20%" property="lesson.course.name" title="attr.courseName"]${apply.lesson.course.name?if_exists}[/@]
		[@b.col width="11%" property="courseType.name" title="entity.courseType"]${apply.lesson.courseType.name?if_exists}[/@]
		[@b.col width="17%" property="teachClass.name" title="entity.teachClass"]${apply.lesson.teachClass.name?if_exists}[/@]
		[@b.col width="17%" property="borrower.name" title="申请教师"]${apply.borrower.name?if_exists}[/@]
		[@b.col width="5%" property="attendance" title="上课人数"/]
    [@b.col width="11%" title="实验室申请"][@b.a href="!info?apply.id=${apply.id}"]查看[/@][/@]
	[/@]
[/@]